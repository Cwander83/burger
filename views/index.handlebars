<h2>Cats that are not sleepy!</h2>

<ul>
  {{#each burgers}}
    {{#unless sleepy}}
      {{> burgers/cat-block sleep=true}}
    {{/unless}}
  {{/each}}
</ul>

<h2>Cats that are sleepy!</h2>

<ul>
  {{#each burgers}}
    {{#if sleepy}}
      {{> burgers/cat-block sleep=false}}
    {{/if}}
  {{/each}}
</ul>
